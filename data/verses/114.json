{"verses":[{"id":6231,"verseNumber":1,"verseKey":"114:1","arabic":" قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ","english":"Say, ˹O Prophet,˺ “I seek refuge in the Lord of humankind,"},{"id":6232,"verseNumber":2,"verseKey":"114:2","arabic":"مَلِكِ ٱلنَّاسِ","english":"the Master of humankind,"},{"id":6233,"verseNumber":3,"verseKey":"114:3","arabic":"إِلَـٰهِ ٱلنَّاسِ","english":"the God of humankind,"},{"id":6234,"verseNumber":4,"verseKey":"114:4","arabic":"مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ","english":"from the evil of the lurking whisperer—"},{"id":6235,"verseNumber":5,"verseKey":"114:5","arabic":"ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ","english":"who whispers into the hearts of humankind—"},{"id":6236,"verseNumber":6,"verseKey":"114:6","arabic":"مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ","english":"from among jinn and humankind.”"}]}
