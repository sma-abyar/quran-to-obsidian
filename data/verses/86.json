[
  {
    "id": 86001,
    "verseNumber": 1,
    "verseKey": "86:1",
    "arabic": "وَ السَّماءِ وَ الطَّارِقِ",
    "persian": "سوگند به آسمان و آن اختر شبگرد."
  },
  {
    "id": 86002,
    "verseNumber": 2,
    "verseKey": "86:2",
    "arabic": "وَ ما أَدْراکَ مَا الطَّارِقُ",
    "persian": "و تو چه دانی که اختر شبگرد چیست؟"
  },
  {
    "id": 86003,
    "verseNumber": 3,
    "verseKey": "86:3",
    "arabic": "النَّجْمُ الثَّاقِبُ",
    "persian": "آن اختر فروزان."
  },
  {
    "id": 86004,
    "verseNumber": 4,
    "verseKey": "86:4",
    "arabic": "إِنْ کُلُّ نَفْسٍ لَمَّا عَلَیْها حافِظٌ",
    "persian": "هیچ کس نیست مگر اینکه نگاهبانی بر او [گماشته شده‌] است."
  },
  {
    "id": 86005,
    "verseNumber": 5,
    "verseKey": "86:5",
    "arabic": "فَلْیَنْظُرِ الْإِنْسانُ مِمَّ خُلِقَ",
    "persian": "پس انسان باید بنگرد که از چه آفریده شده است؟"
  },
  {
    "id": 86006,
    "verseNumber": 6,
    "verseKey": "86:6",
    "arabic": "خُلِقَ مِنْ ماءٍ دافِقٍ",
    "persian": "از آب جهنده‌ای خلق شده،"
  },
  {
    "id": 86007,
    "verseNumber": 7,
    "verseKey": "86:7",
    "arabic": "یَخْرُجُ مِنْ بَیْنِ الصُّلْبِ وَ التَّرائِبِ",
    "persian": "[که‌] از صلب مرد و میان استخوانهای سینه زن بیرون می‌آید."
  },
  {
    "id": 86008,
    "verseNumber": 8,
    "verseKey": "86:8",
    "arabic": "إِنَّهُ عَلى‏ رَجْعِهِ لَقادِرٌ",
    "persian": "در حقیقت، او [= خدا] بر بازگردانیدن وی بخوبی تواناست."
  },
  {
    "id": 86009,
    "verseNumber": 9,
    "verseKey": "86:9",
    "arabic": "یَوْمَ تُبْلَى السَّرائِرُ",
    "persian": "آن روز که رازها [همه‌] فاش شود،"
  },
  {
    "id": 86010,
    "verseNumber": 10,
    "verseKey": "86:10",
    "arabic": "فَما لَهُ مِنْ قُوَّةٍ وَ لا ناصِرٍ",
    "persian": "پس او را نه نیرویی ماند و نه یاری."
  },
  {
    "id": 86011,
    "verseNumber": 11,
    "verseKey": "86:11",
    "arabic": "وَ السَّماءِ ذاتِ الرَّجْعِ",
    "persian": "سوگند به آسمان بارش‌انگیز،"
  },
  {
    "id": 86012,
    "verseNumber": 12,
    "verseKey": "86:12",
    "arabic": "وَ الْأَرْضِ ذاتِ الصَّدْعِ",
    "persian": "سوگند به زمین شکافدار [آماده کشت‌]،"
  },
  {
    "id": 86013,
    "verseNumber": 13,
    "verseKey": "86:13",
    "arabic": "إِنَّهُ لَقَوْلٌ فَصْلٌ",
    "persian": "[که‌] در حقیقت، قرآن گفتاری قاطع و روشنگر است؛"
  },
  {
    "id": 86014,
    "verseNumber": 14,
    "verseKey": "86:14",
    "arabic": "وَ ما هُوَ بِالْهَزْلِ",
    "persian": "و آن شوخی نیست."
  },
  {
    "id": 86015,
    "verseNumber": 15,
    "verseKey": "86:15",
    "arabic": "إِنَّهُمْ یَکیدُونَ کَیْداً",
    "persian": "آنان دست به نیرنگ می‌زنند."
  },
  {
    "id": 86016,
    "verseNumber": 16,
    "verseKey": "86:16",
    "arabic": "وَ أَکیدُ کَیْداً",
    "persian": "و [من نیز] دست به نیرنگ می‌زنم."
  },
  {
    "id": 86017,
    "verseNumber": 17,
    "verseKey": "86:17",
    "arabic": "فَمَهِّلِ الْکافِرینَ أَمْهِلْهُمْ رُوَیْداً",
    "persian": "پس کافران را مهلت ده، و کمی آنان را به حال خود واگذار."
  }
]
