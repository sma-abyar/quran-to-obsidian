[
  {
    "id": 94001,
    "verseNumber": 1,
    "verseKey": "94:1",
    "arabic": "أَ لَمْ نَشْرَحْ لَکَ صَدْرَکَ",
    "persian": "آیا برای تو سینه‌ات را نگشاده‌ایم؟"
  },
  {
    "id": 94002,
    "verseNumber": 2,
    "verseKey": "94:2",
    "arabic": "وَ وَضَعْنا عَنْکَ وِزْرَکَ",
    "persian": "و بار گرانت را از [دوش‌] تو برنداشتیم؟"
  },
  {
    "id": 94003,
    "verseNumber": 3,
    "verseKey": "94:3",
    "arabic": "الَّذی أَنْقَضَ ظَهْرَکَ",
    "persian": "[باری‌] که [گویی‌] پشت تو را شکست."
  },
  {
    "id": 94004,
    "verseNumber": 4,
    "verseKey": "94:4",
    "arabic": "وَ رَفَعْنا لَکَ ذِکْرَکَ",
    "persian": "و نامت را برای تو بلند گردانیدیم."
  },
  {
    "id": 94005,
    "verseNumber": 5,
    "verseKey": "94:5",
    "arabic": "فَإِنَّ مَعَ الْعُسْرِ یُسْراً",
    "persian": "پس [بدان که‌] با دشواری، آسانی است."
  },
  {
    "id": 94006,
    "verseNumber": 6,
    "verseKey": "94:6",
    "arabic": "إِنَّ مَعَ الْعُسْرِ یُسْراً",
    "persian": "آری، با دشواری، آسانی است."
  },
  {
    "id": 94007,
    "verseNumber": 7,
    "verseKey": "94:7",
    "arabic": "فَإِذا فَرَغْتَ فَانْصَبْ",
    "persian": "پس چون فراغت یافتی، به طاعت درکوش؛"
  },
  {
    "id": 94008,
    "verseNumber": 8,
    "verseKey": "94:8",
    "arabic": "وَ إِلى‏ رَبِّکَ فَارْغَبْ",
    "persian": "و با اشتیاق، به سوی پروردگارت روی آور."
  }
]
