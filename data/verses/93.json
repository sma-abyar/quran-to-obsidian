[
  {
    "id": 93001,
    "verseNumber": 1,
    "verseKey": "93:1",
    "arabic": "وَ الضُّحى‏",
    "persian": "سوگند به روشنایی روز،"
  },
  {
    "id": 93002,
    "verseNumber": 2,
    "verseKey": "93:2",
    "arabic": "وَ اللَّیْلِ إِذا سَجى‏",
    "persian": "سوگند به شب چون آرام گیرد،"
  },
  {
    "id": 93003,
    "verseNumber": 3,
    "verseKey": "93:3",
    "arabic": "ما وَدَّعَکَ رَبُّکَ وَ ما قَلى‏",
    "persian": "[که‌] پروردگارت تو را وانگذاشته، و دشمن نداشته است."
  },
  {
    "id": 93004,
    "verseNumber": 4,
    "verseKey": "93:4",
    "arabic": "وَ لَلْآخِرَةُ خَیْرٌ لَکَ مِنَ الْأُولى‏",
    "persian": "و قطعاً آخرت برای تو از دنیا نیکوتر خواهد بود."
  },
  {
    "id": 93005,
    "verseNumber": 5,
    "verseKey": "93:5",
    "arabic": "وَ لَسَوْفَ یُعْطیکَ رَبُّکَ فَتَرْضى‏",
    "persian": "و بزودی پروردگارت تو را عطا خواهد داد، تا خرسند گردی."
  },
  {
    "id": 93006,
    "verseNumber": 6,
    "verseKey": "93:6",
    "arabic": "أَ لَمْ یَجِدْکَ یَتیماً فَآوى‏",
    "persian": "مگر نه تو را یتیم یافت، پس پناه داد؟"
  },
  {
    "id": 93007,
    "verseNumber": 7,
    "verseKey": "93:7",
    "arabic": "وَ وَجَدَکَ ضَالاًّ فَهَدى‏",
    "persian": "و تو را سرگشته یافت، پس هدایت کرد؟"
  },
  {
    "id": 93008,
    "verseNumber": 8,
    "verseKey": "93:8",
    "arabic": "وَ وَجَدَکَ عائِلاً فَأَغْنى‏",
    "persian": "و تو را تنگدست یافت و بی‌نیاز گردانید؟"
  },
  {
    "id": 93009,
    "verseNumber": 9,
    "verseKey": "93:9",
    "arabic": "فَأَمَّا الْیَتیمَ فَلا تَقْهَرْ",
    "persian": "و اما [تو نیز به پاس نعمت ما] یتیم را میازار،"
  },
  {
    "id": 93010,
    "verseNumber": 10,
    "verseKey": "93:10",
    "arabic": "وَ أَمَّا السَّائِلَ فَلا تَنْهَرْ",
    "persian": "و گدا را مران،"
  },
  {
    "id": 93011,
    "verseNumber": 11,
    "verseKey": "93:11",
    "arabic": "وَ أَمَّا بِنِعْمَةِ رَبِّکَ فَحَدِّثْ",
    "persian": "و از نعمت پروردگار خویش [با مردم‌] سخن گوی."
  }
]
